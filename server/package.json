    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-handlebars": "latest",
    "express-rate-limit": "latest",
    "express-validator": "^7.2.1",
    "googleapis": "^148.0.0",
    "helmet": "latest",
    "input-otp": "1.4.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "mysql2": "^3.14.1",
    "node-cache": "^5.1.2",
    "nodemailer": "^7.0.3",
    "nodemailer-express-handlebars": "^7.0.0",
    "nodemailer-handlebars": "^1.0.1",
    "pdfkit": "^0.17.1",
    "sequelize": "latest",
    "sonner": "^1.7.1",
    "stream": "latest",
    "supertest": "latest",
    "url": "latest",
    "util": "latest",
    "uuid": "^11.1.0",
    "vaul": "^0.9.6",
