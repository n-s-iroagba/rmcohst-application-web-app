  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "NODE_ENV=production node dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
   "seed:roles": "ts-node src/seeders/seed-roles.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rmcohst/rmcohst-application-web-app.git"
